<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#2F3642"
                android:endColor="#4B5562"
                android:angle="0"/>
            <corners android:radius="18dp"/>
            <padding
                android:left="6dp"
                android:right="6dp"
                android:top="6dp"
                android:bottom="6dp"/>
        </shape>
    </item>
</layer-list>
